<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd
       ">
       
    <description>xhqbProfile DemoService  dubbo server</description>

    <!-- Application name -->
    <dubbo:application id="xhqbProfileDemoServiceApplication"
                       name="${xhqbProfileDemoService.dubbo.application.name}"
                       owner="${xhqbProfileDemoService.dubbo.application.owner}"
                       organization="${xhqbProfileDemoService.dubbo.application.organization}"
                       logger="slf4j"
                       compiler="javassist"/>


    <dubbo:protocol id="xhqbProfileDemoServiceProtocol"
                    name="${xhqbProfileDemoService.dubbo.protocol.name}"
                    port="-1"
                    dispatcher="${xhqbProfileDemoService.dubbo.protocol.dispatcher}"
                    threadpool="${xhqbProfileDemoService.dubbo.protocol.threadpool}"
                    threads="${xhqbProfileDemoService.dubbo.protocol.threads}">
    </dubbo:protocol>

    <dubbo:registry id="xhqbProfileDemoServiceRegistry"
                    protocol="${xhqbProfileDemoService.dubbo.registry.name}"
                    address="${xhqbProfileDemoService.dubbo.registry.address}"
                    client="${xhqbProfileDemoService.dubbo.registry.client}"
                    group="${xhqbProfileDemoService.dubbo.registry.group}"
                    session="60000"
                    register="true"
                    subscribe="false"
                    check="true"
                    file="xhqbProfileDemoService-registry-cache.properties">
    </dubbo:registry>

     
    <dubbo:provider id="xhqbProfileDemoServiceProvider"
                    application="xhqbProfileDemoServiceApplication"
                    registry="xhqbProfileDemoServiceRegistry"
                    protocol="xhqbProfileDemoServiceProtocol"
                    cluster="${xhqbProfileDemoService.dubbo.provider.cluster}"
                    loadbalance="${xhqbProfileDemoService.dubbo.provider.loadbalance}"
                    serialization="${xhqbProfileDemoService.dubbo.provider.serialization}"
                    retries="${xhqbProfileDemoService.dubbo.provider.retries}"
                    timeout="${xhqbProfileDemoService.dubbo.provider.timeout}"
                    proxy="javassist"
                    delay="-1"
                    filter="elapsedTimeFilter"
                    >
    </dubbo:provider>
</beans>