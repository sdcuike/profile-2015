<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <description>doctor2015BusinessClient dubbo client</description>

    <dubbo:application id="xhqbProfileDemoClientApplication"
                       name="${xhqbProfileDemoClient.dubbo.application.name}"
                       owner="${xhqbProfileDemoClient.dubbo.application.owner}"
                       organization="${xhqbProfileDemoClient.dubbo.application.organization}"
                       logger="slf4j"
                       compiler="javassist"/>
    
                    
    <dubbo:registry id="xhqbProfileDemoClientRegistry"
                    protocol="${xhqbProfileDemoClient.dubbo.registry.name}"
                    client="${xhqbProfileDemoClient.dubbo.registry.client}"
                    address="${xhqbProfileDemoClient.dubbo.registry.address}"
                    group="${xhqbProfileDemoClient.dubbo.registry.group}"
                    check="${xhqbProfileDemoClient.dubbo.consumer.check}"
                    subscribe="true"
                    file="xhqbProfileDemoClient-registry-cache.properties">
    </dubbo:registry>

    <dubbo:consumer id="xhqbProfileDemoClientConsumer"
                    application="xhqbProfileDemoClientApplication"
                    registry="xhqbProfileDemoClientRegistry"
                    init="false"
                    check="${xhqbProfileDemoClient.dubbo.consumer.check}"
                    filter="elapsedTimeFilter"
                    timeout="${xhqbProfileDemoClient.dubbo.consumer.timeout}"
                    retries="0"/>
                 
</beans>